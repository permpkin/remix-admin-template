{
  "name": "remix-press-525a",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:*",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "dev": "bun run-p dev:*",
    "dev:css": "npm run generate:css -- --watch",
    "dev:remix": "cross-env NODE_ENV=development binode -- @remix-run/dev:remix dev",
    "generate:css": "tailwindcss -i ./app/styles/style.css -o ./public/tailwind.css",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "setup": "prisma db push && prisma generate && prisma migrate deploy && prisma db seed",
    "start": "remix-serve build",
    "start:mocks": "binode -- @remix-run/serve:remix-serve build"
  },
  "prettier": {},
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build"
  ],
  "dependencies": {
    "@headlessui/react": "^1.6.6",
    "@headlessui/tailwindcss": "^0.1.1",
    "@heroicons/react": "^2.0.13",
    "@prisma/client": "^4.2.1",
    "@remix-run/node": "^1.6.4",
    "@remix-run/react": "^1.6.4",
    "@remix-run/serve": "^1.6.4",
    "@remix-run/server-runtime": "^1.6.4",
    "@tanstack/react-table": "^8.7.0",
    "ajv": "^8.11.0",
    "ajv-error-messages": "^2.1.0",
    "ajv-formats": "^2.1.1",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "formik": "^2.2.9",
    "http-status-codes": "^2.2.0",
    "isbot": "^3.5.0",
    "liquidjs": "^9.42.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "schema-to-yup": "^1.11.16",
    "tiny-invariant": "^1.2.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.6.4",
    "@remix-run/eslint-config": "^1.6.4",
    "@tailwindcss/forms": "^0.5.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.2",
    "@types/eslint": "^8.4.5",
    "@types/node": "^18.0.3",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^2.0.0",
    "autoprefixer": "^10.4.7",
    "binode": "^1.0.5",
    "c8": "^7.11.3",
    "cross-env": "^7.0.3",
    "cypress": "^10.3.0",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "happy-dom": "^6.0.3",
    "msw": "^0.44.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "prettier": "2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.12",
    "prisma": "^4.2.1",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "^3.1.6",
    "ts-node": "^10.8.2",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.4",
    "vite": "^3.0.0",
    "vite-tsconfig-paths": "^3.5.0",
    "vitest": "^0.18.0"
  },
  "engines": {
    "node": ">=14"
  },
  "prisma": {
    "seed": "ts-node --require tsconfig-paths/register prisma/seed.ts"
  }
